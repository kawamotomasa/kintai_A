
<h1>拠点新規画面</h1>
<%= form_with(model: Bases, url:bases_path,method: "post", local: true) do |f| %>
 <%= f.label :拠点番号, class: "label-#{yield(:class_text)}" %>
 <%= f.text_field :office_no, class: "form-control" %>

 <%= f.label :拠点名, class: "label-#{yield(:class_text)}" %>
 <%= f.text_field :office_name, class: "form-control" %>
  
 <%= f.label :勤怠種類, class: "label-#{yield(:class_text)}" %>
 <%= f.text_field :work_kind, class: "form-control" %></br>
 <%= f.submit "新規作成", class: "btn btn-primary btn-block " %>
<% end %>

